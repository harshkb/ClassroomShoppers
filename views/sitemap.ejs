<!DOCTYPE html>
<html>
<head>
<!-- Basic -->
<meta charset="utf-8">
<title>Classroom Shoppers - Sitemap</title>
<meta name="keywords" content="Online Shopping" />
<meta name="description" content="DBMS Course project">
<meta name="author" content="etheme.com">
<link rel="shortcut icon" href="/../images/favicon.ico">
<!-- Mobile Specific Metas -->
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<!-- Web Fonts  -->
<link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800%7CShadows+Into+Light" rel="stylesheet" type="text/css">
<!-- Icon Fonts  -->
<link rel="stylesheet" href="/../font/style.css">
<!-- /../Vendor CSS -->
<link rel="stylesheet" href="/../vendor/waves/waves.css">
<link rel="stylesheet" href="/../vendor/slick/slick.css">
<link rel="stylesheet" href="/../vendor/slick/slick-theme.css">
<link rel="stylesheet" href="/../vendor/bootstrap-select/bootstrap-select.css">
<!-- Custom CSS -->
<link rel="stylesheet" href="/../css/style.css">
<link rel="stylesheet" href="/../css/style-colors.css">
<!-- Custom Fonts -->
<link href='https://fonts.googleapis.com/css?family=Roboto:400,300,300italic,400italic,700,700italic' rel='stylesheet' type='text/css'>
<link href='https://fonts.googleapis.com/css?family=Ubuntu:400,700' rel='stylesheet' type='text/css'>

<!-- Head Libs -->

<!--[if IE]>
        <link rel="stylesheet" href="css/ie.css">
        <![endif]-->
<!--[if lte IE 8]>
        <script src="http://cdnjs.cloudflare.com/ajax/libs/respond.js/1.3.0/respond.js"></script>
        <script src="http://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7/html5shiv.js"></script>
        <![endif]-->

<!-- Modernizr -->
<script src="/../vendor/modernizr/modernizr.js"></script>
</head>

<body>
<div id="loader-wrapper" class="loader-off">
<div id="loader"></div>
</div>

<%- include search %>

<div class="wrapper">
  <!-- Header section -->
  <%- include header %>
  
  <div id="pageContent">

    <!-- Breadcrumb section -->

    <section class="breadcrumbs  hidden-xs">
      <div class="container">
        <ol class="breadcrumb breadcrumb--wd pull-left">
          <li><a href="#">Home</a></li>
          <li class="active">Sitemap</li>
        </ol>
      </div>
    </section>

    <!-- Content section -->
    <section class="content">
      <div class="container">
        <h2 class="text-uppercase">sitemap</h2>
        <div class="panel-group" id="sitemap">
        <% for(var i = 0;i < categ.length;i++) { %>
          <div class="panel panel-default" role="tablist">
            <div class="panel-heading" role="tab">
              <h4 class="panel-title"> <a role="button" data-toggle="collapse" href="#collapseOne"> <%=categ[i].categoryName%> </a> </h4>
            </div>
            <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel">
              <div class="panel-body">
                <div class="row">
                  <div class="col-sm-6 col-md-3">
                    <ul class="simple-list">
                     <% for(var j = 0;j < subcateg.length;j++) { %>
                     <% if(subcateg[j].Category_id==categ[i].Category_id){%>    
                      <li><a href="/user/<%=categ[i].Category_id%>/<%=user_id%>"><%=subcateg[j].subCategoryName%></a></li>
                      <% } %>
                      <% } %>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <% } %>
          
          <div class="panel panel-default" role="tablist">
            <div class="panel-heading" role="tab">
              <h4 class="panel-title"> <a role="button" data-toggle="collapse" href="#collapseSix"> My Account </a> </h4>
            </div>
            <div id="collapseSix" class="panel-collapse collapse in" role="tabpanel">
              <div class="panel-body">
                <div class="row">
                  <div class="col-sm-6 col-md-3">
                    <ul class="simple-list">
                      <li><a href="/account-order/user/<%=user_id%>"> My Account</a></li>
                      <li><a href="/account-order/user/<%=user_id%>"> Order history</a></li>
                    </ul>
                  </div>
                  <div class="col-sm-6 col-md-3">
                    <ul class="simple-list">
                      <li><a href="#"> Advanced search</a></li>
                      <li><a href="#"> Reviews</a></li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="panel panel-default" role="tablist">
            <div class="panel-heading" role="tab">
              <h4 class="panel-title"> <a role="button" data-toggle="collapse" href="#collapseSeven"> customer service </a> </h4>
            </div>
            <div id="collapseSeven" class="panel-collapse collapse in" role="tabpanel">
              <div class="panel-body">
                <div class="row">
                  <div class="col-sm-6 col-md-3">
                    <ul class="simple-list">
                      <li><a href="#"> Online support</a></li>
                      <li><a href="/faq/user/<%=user_id%>"> Help & FAQs</a></li>
                    </ul>
                  </div>
                  <div class="col-sm-6 col-md-3">
                    <ul class="simple-list">
                      <li><a href="#"> Call Center</a></li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="panel panel-default" role="tablist">
            <div class="panel-heading" role="tab">
              <h4 class="panel-title"> <a role="button" data-toggle="collapse" href="#collapseEight"> Information </a> </h4>
            </div>
            <div id="collapseEight" class="panel-collapse collapse in" role="tabpanel">
              <div class="panel-body">
                <div class="row">
                  <div class="col-sm-6 col-md-3">
                    <ul class="simple-list">
                      <li><a href="/about/user/<%=user_id%>">About Us</a></li>
                      <li><a href="#">Shipping & Returns</a></li>
                    </ul>
                  </div>
                  <div class="col-sm-6 col-md-3">
                    <ul class="simple-list">
                      <li><a href="#"> Privacy Notice</a></li>
                      <li><a href="#"> Conditions of Use</a></li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- End Content section -->
  </div>

  <%- include footer %>
</div>
<!-- /../Vendor -->
<!-- jQuery 1.10.1-->
<script src="/../vendor/jquery/jquery.js"></script>
<!-- Bootstrap 3-->
<script src="/../vendor/bootstrap/bootstrap.min.js"></script>
<!-- Specific Page /../Vendor -->
<script src="/../vendor/waves/waves.min.js"></script>
<script src="/../vendor/slick/slick.min.js"></script>
<script src="/../vendor/bootstrap-select/bootstrap-select.min.js"></script>
<script src="/../vendor/parallax/jquery.parallax-1.1.3.js"></script>
<script src="/../vendor/waypoints/jquery.waypoints.min.js"></script>
<script src="/../vendor/waypoints/sticky.min.js"></script>
<script src="/../vendor/doubletaptogo/doubletaptogo.js"></script>
<script src="/../vendor/elevatezoom/jquery.elevatezoom.js"></script>
<script src="/../vendor/imagesloaded/imagesloaded.pkgd.min.js"></script><script src="/../vendor/countdown/jquery.plugin.min.js"></script>
<script src="/../vendor/countdown/jquery.countdown.min.js"></script>
<!-- jQuery form validation -->
<script src="/../vendor/form/jquery.form.js"></script>
<script src="/../vendor/form/jquery.validate.min.js"></script>
<!-- Custom -->
<script src="/../js/custom.js"></script>
</body>
</html>
